set nocompatible
syntax on
colorscheme desert
filetype on
set showmatch " show matching brackets/paranthesis
set tabstop=4
set number
set autoindent
set shiftwidth=4
filetype indent plugin on
set pastetoggle=<F12>
set clipboard=unnamed
set mouse =a
set bs=2 "make backspace behave like normal
vnoremap < <gv " better indentation
vnoremap > >gv " better indentation


